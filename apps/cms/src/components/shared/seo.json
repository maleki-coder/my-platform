{
  "collectionName": "components_shared_seos",
  "info": {
    "name": "Seo",
    "icon": "allergies",
    "displayName": "Seo",
    "description": ""
  },
  "options": {},
  "attributes": {
    "seo_title": {
      "type": "string",
      "required": true,
      "maxLength": 60
    },
    "seo_description": {
      "type": "text",
      "required": true,
      "maxLength": 160
    },
    "canonical_url": {
      "type": "string"
    },
    "robots": {
      "type": "enumeration",
      "enum": [
        "index, follow",
        "index, nofollow",
        "noindex, follow",
        "noindex, nofollow"
      ],
      "default": "index, follow"
    },
    "prevent_indexing": {
      "type": "boolean",
      "default": false
    },
    "keywords": {
      "type": "string"
    },
    "og_title": {
      "type": "string",
      "maxLength": 70
    },
    "og_description": {
      "type": "text",
      "maxLength": 200
    },
    "og_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "twitter_title": {
      "type": "string"
    },
    "twitter_description": {
      "type": "text"
    },
    "twitter_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "twitter_card_type": {
      "type": "enumeration",
      "enum": [
        "summary",
        "summary_large_image",
        "app",
        "player"
      ],
      "default": "summary_large_image"
    },
    "schema_json": {
      "type": "json"
    },
    "meta_json_extra": {
      "type": "json"
    },
    "priority": {
      "type": "decimal",
      "min": 0,
      "max": 1,
      "default": 0.5
    },
    "change_frequency": {
      "type": "enumeration",
      "enum": [
        "always",
        "hourly",
        "daily",
        "weekly",
        "monthly",
        "yearly",
        "never"
      ],
      "default": "daily"
    }
  }
}